{
  "name": "create-nodejs-express-app",
  "version": "1.7.0",
  "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
  "bin": {
    "create-nodejs-express-app": "bin/createNodejsApp.js"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hagopj13/node-express-boilerplate.git"
  },
  "author": "Hagop Jamkojian <hagopj13@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build:digitalocean": "yarn install --production=false && yarn run build && rm -rf node_modules && yarn install --production --frozen-lockfile",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@socket.io/mongo-adapter": "^0.3.0",
    "agenda": "^5.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron-time-generator": "^1.3.2",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "he": "^1.2.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "moment": "^2.24.0",
    "mongodb": "^5.1.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.9",
    "nodemailer": "^6.3.1",
    "openapi-types": "12.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.3.0",
    "regenerator-runtime": "^0.13.11",
    "sequelize": "^6.28.0",
    "socket.io": "^4.6.1",
    "socket.io-adapter": "^2.5.2",
    "soft-delete-plugin-mongoose": "^1.0.15",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^9.0.0",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.1381.0",
    "coveralls": "^3.0.7",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.9.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "supertest": "^6.0.1"
  },
  "bugs": {
    "url": "https://github.com/hagopj13/node-express-boilerplate/issues"
  },
  "homepage": "https://github.com/hagopj13/node-express-boilerplate#readme",
  "directories": {
    "test": "tests"
  }
}
